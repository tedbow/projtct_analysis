{
    "name": "drupalorg_infrastructure/project_analysis_utils",
    "description": "Internal library for utilities",
    "type": "library",
    "require": {
        "symfony/yaml": "*",
        "composer/semver": "*",
        "composer/composer": "*"
    },
    "bin": [
        "bin/rector_needed",
        "bin/info_updatable",
        "bin/update_info",
        "bin/restore_nonrector_changes",
        "bin/test_error",
        "bin/find_machinename",
        "bin/composer_json_updatable",
        "bin/update_composer_json"
    ],
    "autoload": {
        "psr-4": {
            "InfoUpdater\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InfoUpdater\\Tests\\": "tests/src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.1"
    }
}
